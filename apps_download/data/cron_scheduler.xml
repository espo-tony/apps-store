<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record forcecreate="True" id="ir_cron_scheduler_generate_zip" model="ir.cron">
            <field name="name">Generate all product zip</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field eval="'product.product'" name="model"/>
            <field eval="'generate_zip_file_batch'" name="function"/>
            <field name="args" eval="'()'" />
        </record>

    </data>
</odoo>
