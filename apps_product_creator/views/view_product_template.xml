<odoo>
    <record id="product_template_form_view" model="ir.ui.view">
        <field name="name">product.template.form (in github_product_creator)</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Module Informations" name="module_informations" attrs="{'invisible':[('odoo_module_id','=',False)]}">
                    <group>
                        <group>
                            <field name="odoo_module_id"/>
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
